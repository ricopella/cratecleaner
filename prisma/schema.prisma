generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CrateSrc {
  id   Int    @id @default(autoincrement())
  path String
  type String
}

model FilesDirectory {
  id   Int    @id @default(autoincrement())
  path String @unique
}
