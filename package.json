{
  "name": "create-cleaner",
  "version": "1.0.0",
  "description": "Cleanup your crates & files",
  "main": "./out/main/index.js",
  "author": "Narin Rico Sundarabhaya",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "build:linux": "NODE_ENV=development && electron-vite build && electron-builder --linux --config",
    "build:mac": "NODE_ENV=development && electron-vite build && electron-builder --mac --config",
    "build:win": "NODE_ENV=development && npm run build && electron-builder --win --config",
    "build": "NODE_ENV=development && npm run typecheck && electron-vite build",
    "dev": "NODE_ENV=development && electron-vite dev",
    "format": "prettier --write .",
    "generate": "prisma generate",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "postinstall": "electron-builder install-app-deps",
    "predev": "prisma migrate dev",
    "start": "electron-vite preview",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "test": "jest --coverage --verbose"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@prisma/client": "^5.3.1",
    "@tanstack/react-table": "^8.10.3",
    "daisyui": "^3.8.2",
    "date-fns": "^2.30.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-updater": "^6.1.1",
    "framer-motion": "^10.16.4",
    "music-metadata": "7.13.4",
    "ramda": "^0.29.0",
    "tailwindcss": "^3.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.5",
    "@types/ramda": "^0.29.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.2",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.1.0"
  }
}
